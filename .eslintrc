{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [0],
        "curly": [2],
        "eqeqeq": [2],
        "no-alert": [2],
        "no-eval": [2],
        "no-extra-bind": [2],
        "no-floating-decimal": [2],
        "no-implicit-coercion": [1],
        "no-implied-eval": [2],
        "no-invalid-this": [1],
        "no-iterator": [2],
        "no-lone-blocks": [2],
        "no-loop-func": [1],
        "no-multi-spaces": [2],
        "no-multi-str": [2],
        "no-native-reassign": [2],
        "no-new-func": [2],
        "no-new-wrappers": [2],
        "no-new": [2],
        "no-param-reassign": [2],
        "no-process-env": [2],
        "no-proto": [2],
        "no-return-assign": [2],
        "no-script-url": [2],
        "no-self-compare": [2],
        "no-sequences": [2],
        "no-throw-literal": [2],
        "no-unused-expressions": [2],
        "no-useless-call": [1],
        "no-useless-concat": [2],
        "no-void": [2],
        "no-with": [2],
        "radix": [2],
        "vars-on-top": [1],
        "wrap-iife": [1]
    },
    "env": {
        "browser": true
    },
     "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "modules": true,
        "superInFunctions": true,
        "templateStrings": true
    },
    "globals": {
    	"lscache": true,
    	"Ractive": true,
    	"PSRR": true
    },
    "extends": "eslint:recommended"
}